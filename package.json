    "production": "cross-env NODE_ENV=production postcss assets/css/styles.css -o assets/css/styles.min.css",
    "deploy": "npm run production"
  },
  "dependencies": {
    "autoprefixer": "^9.8.6",
    "browser-sync": "2.24.6",
    "tailwindcss": "^1.7.2",
    "concurrently": "^6.3.0",
    "cross-env": "^7.0.3",
    "cssnano": "^5.0.8",
    "live-server": "git://github.com/tapio/live-server.git#5c4539d0816eb6f30a4020444c093415dceeb5eb",
